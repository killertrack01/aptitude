
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
}
@section CSS{
    <link rel="stylesheet" href="~/css/thanhcss.css">
}
<div class="container">
    <!--Title-->
    <div class="row">
        <div class="col-lg-12">
            <h1>List all Department</h1>
        </div>
    </div>
    <!--Search-->
    <div class="row">
        <div class="col-lg-8"></div>
        <div class="col-lg-4">
            <form asp-action="PartialView/Department_Index" asp-controller="Admin" method="get">
                <partial name="PartialView/_SearchDepartment" />
            </form>
        </div>
    </div>
    <!---Table--->
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <partial name="PartialView/_TableDepartment" />
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script src="~/js/thanhjs.js"></script>
}